{
  "name": "example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/server/renderer.tsx --outDir api",
    "preview": "NODE_ENV=production bun run src/server/server.ts",
    "serve:ssr": "bun run src/server/watcher.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview:old": "vite preview"
  },
  "dependencies": {
    "@edge-runtime/ponyfill": "^3.0.0",
    "@edge-runtime/types": "^3.0.1",
    "edge-runtime": "^3.0.1",
    "express": "^4.19.2",
    "react": "^18.3",
    "react-dom": "^18.3"
  },
  "devDependencies": {
    "@swc/core": "^1.7.0-nightly-20240714.1",
    "@swc/helpers": "^0.5.12",
    "@swc/plugin-relay": "^2.0.11",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "compression": "^1.7.4",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "sirv": "^2.0.4",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.3.1"
  }
}
