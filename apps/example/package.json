{
  "name": "example",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "browserslist": [
    "last 2 Chrome versions"
  ],
  "scripts": {
    "dev": "vite",
    "build": "bun run build:client && bun run build:server",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/server/renderer.tsx --outDir api",
    "preview": "NODE_ENV=production bun run --inspect src/server/server.ts",
    "serve:ssr": "bun run src/server/watcher.ts",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview:old": "vite preview"
  },
  "dependencies": {
    "htmlparser2": "^9.1.0",
    "react": "^18.3",
    "react-dom": "^18.3"
  },
  "devDependencies": {
    "@aztlan/grid": "^0.10.0",
    "@aztlan/stylelint-config": "^0.10.0",
    "@changesets/cli": "^2.27.7",
    "@swc/core": "^1.7.0-nightly-20240714.1",
    "@swc/helpers": "^0.5.12",
    "@swc/plugin-relay": "^2.0.11",
    "@types/express": "^4.17.21",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "browserlist": "^1.0.1",
    "compression": "^1.7.4",
    "eslint": "^9.7.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "lightningcss": "^1.25.1",
    "sass": "^1.77.8",
    "sirv": "^2.0.4",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.3.1",
    "vite-plugin-raw": "^1.0.3"
  },
  "stylelint": {
    "extends": "@aztlan/stylelint-config"
  }
}
